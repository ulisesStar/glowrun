.ordenes(ng-init="obtenerOrdenes()")
    md-card
        md-table-container
            table(md-table md-row-select multiple)
                thead(md-head)
                    tr(md-row)
                        th(md-column md-numeric md-order-by="orden.createdAt")
                            md-icon today
                        th(md-column md-numeric) Carrera
                        th(md-column md-numeric) Cantidad
                        th(md-column md-numeric) Status
                tbody(md-body)
                    tr(md-row md-select="dato" md-select-id="name" md-auto-select ng-repeat="orden in ordenes")
                        td(md-cell)
                            strong {{orden.createdAt | date:'yyyy-MMMM-dd HH:mm:ss'}}
                        td(md-cell) {{orden.carrera.nombre}}
                        td(md-cell) {{orden.cantidad}}
                        td(md-cell) {{orden.status}}
        md-table-pagination(md-page-select)
